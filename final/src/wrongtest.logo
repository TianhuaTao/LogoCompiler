@SIZE 1094 1960
@BACKGROUND 20 20 20 
@POSITION 500  900
COLOR 255 255 255
DEF Z 100


FUNC I ()
LOOP 360
TURN 1
MOVE 3
END LOOP
END FUNC

FUNC H ()
LOOP 360
TURN 1
MOVE 2
END LOOP
CALL I ()
END FUNC

FUNC G ()
