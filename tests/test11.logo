	MOVE 50
	COLOR 0 0 0
	
	TURN 162
	MOVE size TURN -72
	MOVE size TURN 144
	MOVE size TURN  -72
	MOVE size TURN 144
	MOVE size TURN  -72
	MOVE size TURN 144
	MOVE size TURN  -72
	MOVE size TURN 144
	MOVE size TURN  -72
	MOVE size 
	TURN -18
	// MOVE size
END FUNC

