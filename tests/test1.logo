@SIZE 1920 1080
@BACKGROUND 255 255 255
@POSITION 959 10
TURN 90
LOOP 180
    MOVE 2
    TURN -1
END LOOP

LOOP 360
    MOVE 3
    TURN 1
END LOOP
LOOP 180
    MOVE 2
    TURN -1
END LOOP


DEF count 0
FUNC circle(step)
    LOOP 360
        MOVE step
        TURN 1
    END LOOP
    ADD count 1
END FUNC